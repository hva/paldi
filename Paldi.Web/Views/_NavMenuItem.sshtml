@IfNot.HasItems
<li><a href="@Current.Url">@Current.Title</a></li>
@EndIf
@If.HasItems
<li class="dropdown">
    <a href="@Current.Url" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">@Current.Title <span class="caret"></span></a>
    <ul class="dropdown-menu">
        @Each.Items
        <li><a href="@Current.Url">@Current.Title</a></li>
        @EndEach
    </ul>
</li>
@EndIf